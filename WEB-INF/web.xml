<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">
<display-name>Validacion</display-name>
<welcome-file-list>
    <welcome-file>Htmls/formularioEditarIngrediente.html</welcome-file>
</welcome-file-list>

<servlet>
    <servlet-name>formularioEditarIngredienteServlet</servlet-name>
    <servlet-class>/WEB-INF/jsp/formuarioEditarIngrediente.jsp</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>formularioEditarIngredienteServlet</servlet-name>
    <url-pattern>/formuarioEditarIngrediente.jsp</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>ValidacionFormServlet</servlet-name>
    <servlet-class>ValidacionFormServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>ValidacionFormServlet</servlet-name>
    <url-pattern>/validacionFormServlet</url-pattern>
</servlet-mapping>

</web-app>